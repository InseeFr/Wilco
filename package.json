{
	"name": "@inseefr/wilco",
	"version": "0.0.5",
	"repository": {
		"type": "git",
		"url": "https://github.com/InseeFr/wilco"
	},
	"license": "MIT",
	"author": "INSEE (http://www.insee.fr)",
	"main": "dist/index.js",
	"source": "src/index.js",
	"files": [
		"dist",
		"lib",
		"src"
	],
	"scripts": {
		"build": "rollup -c rollup.config.js",
		"build:dev": "rollup -c rollup.config.js -w",
		"start-storybook": "start-storybook -p 9999",
		"build-storybook": "build-storybook -o built-storybook/storybook",
		"test": "jest",
		"test-watch": "jest --watch",
		"test-coverage": "jest --coverage",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"lint": "eslint src",
		"prepublishOnly": "npm run test && npm run build",
		"pre-push": "yarn run test-coverage && yarn run lint"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn pre-push"
		}
	},
	"peerDependencies": {
		"react": "^16.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-modules-commonjs": "^7.8.3",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.8.3",
		"@storybook/addon-actions": "5.3.19",
		"@storybook/addon-knobs": "5.3.19",
		"@storybook/addon-links": "5.3.19",
		"@storybook/addons": "5.3.19",
		"@storybook/react": "5.3.19",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^10.4.7",
		"babel-eslint": "^10.0.3",
		"babel-loader": "8.1.0",
		"bootstrap": "^4.5.0",
		"eslint": "^7.5.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-config-react-app": "5.2.1",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-react": "7.20.5",
		"eslint-plugin-react-hooks": "^4.0.8",
		"husky": "^4.2.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "26.2.1",
		"jest-sonar-reporter": "^2.0.0",
		"node-sass": "^4.13.1",
		"rollup": "^2.23.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-postcss": "^3.1.3",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-watch": "^4.3.1",
		"sass-loader": "^9.0.2"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}",
			"!src/**/*.stories.js"
		],
		"moduleNameMapper": {
			"^.+\\.(css|less|scss)$": "identity-obj-proxy"
		},
		"transform": {
			"^.+\\.js$": "babel-jest"
		},
		"collectCoverage": true,
		"testResultsProcessor": "jest-sonar-reporter",
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/test/"
		]
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	},
	"dependencies": {
		"@textlint/markdown-to-ast": "^6.1.7",
		"dompurify": "^2.0.12",
		"draft-js": "^0.11.6",
		"draft-js-export-html": "^1.4.1",
		"draft-js-import-html": "^1.4.1",
		"lodash.deburr": "^4.1.0",
		"lodash.difference": "^4.5.0",
		"lodash.filter": "^4.6.0",
		"lodash.map": "^4.6.0",
		"prop-types": "^15.7.2",
		"query-string": "^6.9.0",
		"react-bootstrap": "1.3.0",
		"react-bootstrap-table-next": "4.0.3",
		"react-bootstrap-table2-paginator": "2.1.2",
		"react-bootstrap-table2-toolkit": "2.1.3",
		"react-draft-wysiwyg": "^1.14.5",
		"react-loading": "^2.0.3",
		"react-router-dom": "^5.1.2",
		"react-select": "^3.0.4"
	},
	"resolutions": {
		"@babel/preset-env": "7.5.5"
	}
}
