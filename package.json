{
	"name": "@inseefr/wilco",
	"version": "0.0.8",
	"repository": {
		"type": "git",
		"url": "https://github.com/InseeFr/wilco"
	},
	"license": "MIT",
	"author": "INSEE (http://www.insee.fr)",
	"main": "dist/index.js",
	"source": "src/index.js",
	"files": [
		"dist",
		"lib",
		"src"
	],
	"scripts": {
		"build": "rollup -c rollup.config.js",
		"start-storybook": "start-storybook -p 9999",
		"build-storybook": "build-storybook -o built-storybook/storybook",
		"test": "jest",
		"test-watch": "jest --watch",
		"test-coverage": "jest --coverage",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"lint": "eslint src",
		"prepublishOnly": "npm run test && npm run build",
		"pre-push": "yarn run test-coverage && yarn run lint"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn pre-push"
		}
	},
	"peerDependencies": {
		"react": "^16.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-modules-commonjs": "^7.8.3",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.8.3",
		"@storybook/addon-actions": "5.3.13",
		"@storybook/addon-knobs": "5.3.13",
		"@storybook/addon-links": "5.3.13",
		"@storybook/addons": "5.3.13",
		"@storybook/react": "5.3.13",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^9.4.1",
		"babel-eslint": "^10.0.3",
		"babel-loader": "8.0.6",
		"bootstrap": "^3.4.1",
		"coveralls": "^3.0.9",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "6.7.0",
		"eslint-config-react-app": "5.1.0",
		"eslint-plugin-flowtype": "^4.6.0",
		"eslint-plugin-import": "2.19.1",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"husky": "^4.2.3",
		"identity-obj-proxy": "^3.0.0",
		"jest": "24.9.0",
		"rollup": "^1.31.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-postcss": "^2.0.6",
		"rollup-plugin-replace": "^2.2.0",
		"sass-loader": "^8.0.2"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}",
			"!src/**/*.stories.js"
		],
		"moduleNameMapper": {
			"^.+\\.(css|less|scss)$": "identity-obj-proxy"
		},
		"transform": {
			"^.+\\.js$": "babel-jest"
		}
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	},
	"dependencies": {
		"lodash.deburr": "^4.1.0",
		"lodash.difference": "^4.5.0",
		"lodash.filter": "^4.6.0",
		"lodash.map": "^4.6.0",
		"query-string": "^6.9.0",
		"react-bootstrap": "0.31.5",
		"react-bootstrap-table-next": "3.3.3",
		"react-bootstrap-table2-paginator": "2.1.0",
		"react-bootstrap-table2-toolkit": "2.1.1",
		"react-loading": "^2.0.3",
		"react-router-dom": "^5.1.2",
		"react-select": "^3.0.4",
		"sass": "^1.49.9"
	}
}
